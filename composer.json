{
    "name": "motto/wp-blaze-css",
    "description": "WordPress plugin to render your CSS as fast as possible. Crush your lighthouse scores by simply activating the plugin.",
    "type": "wordpress-plugin",
    "keywords": ["wordpress", "plugin"],
    "homepage": "http://motto.ca/",
    "license": "GPL-2.0+",
    "authors": [
      {
          "name": "Greg Hunt",
          "email": "greg@motto.ca",
          "homepage": "https://motto.ca/"
      }
    ],
    "support": {
        "issues": "https://github.com/wpmotto/wp-blaze-css/issues",
        "source": "https://github.com/wpmotto/wp-blaze-css"
    },
    "autoload": {
        "psr-4": {
            "Motto\\BlazeCss\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Motto\\Tests\\BlazeCss\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0",
        "composer/installers": "~1.0",
        "sabberworm/php-css-parser": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "*@stable"
    },
    "suggest": {}
}
